{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "~/albums-page/*": ["src/components/+my-pages/albums/*"],
      "~/album-page/*": ["src/components/+my-pages/album/*"],
      "~/videos-page/*": ["src/components/+my-pages/videos-page/*"],
      "~/about-page/*": ["src/components/+my-pages/about/*"],
      "~/home/*": ["src/components/+my-pages/home/*"],
      "~/images/*": ["src/components/+my-pages/images/*"]
    },
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    ".eslintrc.cjs",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs"
  ],
  "exclude": ["node_modules", "src/pages/test.jsx"]
}
