  "workbench.editor.labelFormat": "short",
  "css.lint.unknownAtRules": "ignore",
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "localhost",
          "commands": ["yarn dev"],
          "shouldRunCommands": false
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "git"
        }
      ]
    }
  ]
}
